{
    "name": "laminas-api-tools/api-tools-content-negotiation",
    "description": "Laminas Module providing content-negotiation features",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "content-negotiation"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-content-negotiation/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-content-negotiation",
        "rss": "https://github.com/laminas-api-tools/api-tools-content-negotiation/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas/laminas-eventmanager": "~2.3",
        "laminas/laminas-filter": "~2.3",
        "laminas/laminas-json": "~2.3",
        "laminas/laminas-mvc": "~2.3",
        "laminas/laminas-servicemanager": "~2.3",
        "laminas/laminas-stdlib": "~2.3",
        "laminas/laminas-validator": "~2.3",
        "laminas/laminas-view": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "laminas-api-tools/api-tools-hal": "~1.0",
        "laminas/laminas-http": "~2.3",
        "laminas/laminas-loader": "~2.3",
        "phpunit/phpunit": "3.7.*"
    },
    "suggest": {
        "laminas/laminas-console": "~2.3, if you intend to use the RequestFactory"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\ContentNegotiation\\": "src/",
            "LaminasTest\\ApiTools\\ContentNegotiation\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "replace": {
        "zfcampus/zf-content-negotiation": "self.version"
    }
}
